{
  "extends": "airbnb",
  // Здесь вы указываете, что наследуетесь от набора правил
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "max-len": [
      2,
      100
    ],
    "eol-last": ["error", "always"],
    "indent": ["error", 2],
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "@typescript-eslint/no-unused-vars": 2
  }
}
